CREATE OR REPLACE FUNCTION func_tracker_designer_receive_order_embroidery(
    IN v_order_id integer,
    IN v_embrd_custom_order_id integer,
    OUT output_msg text,
    OUT id integer)
  RETURNS record AS
$BODY$
BEGIN  
                                                               
		UPDATE tracker_custom_received_orders_embroidery
		SET 
		designer_received='Y', 
		designer_received_date=current_timestamp
		WHERE embrd_custom_order_id =v_embrd_custom_order_id;
		output_msg:='Successfully';

               
END
$BODY$
  LANGUAGE plpgsql;
