CREATE TABLE TRACKER_CUSTOM_RECEIVED_ORDERS_EMBROIDERY
(
	embrd_custom_order_id serial NOT NULL UNIQUE,
	order_id int NOT NULL,
	embrd_total_metre float NOT NULL,
	embrd_total_pcs int NOT NULL,
	embrd_total_set int NOT NULL,
	embrd_total_line float NOT NULL,
	embrd_total_length_inch1 float NOT NULL,
	embr_total_width_inch2 float NOT NULL,
	embrd_total_height_inch3 float NOT NULL,
	embrd_total_measurement float NOT NULL,	
	received_date timestamp NOT NULL,
	designer_received char(1),
	designer_received_date timestamp,
	designer_rejected char(1),
	designer_returned char(1),
	designer_not_accepted_reason text,
	PRIMARY KEY (embrd_custom_order_id)
) WITHOUT OIDS;
