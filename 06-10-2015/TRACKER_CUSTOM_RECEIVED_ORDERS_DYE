CREATE TABLE TRACKER_CUSTOM_RECEIVED_ORDERS_DYE
(
	dye_custom_order_id serial NOT NULL UNIQUE,
	order_id int NOT NULL,
	dye_length float NOT NULL,
	dye_width float NOT NULL,
	dye_pcs int NOT NULL,
	dye_total_measurement float NOT NULL,
	received_date timestamp,
	designer_received char(1),
	designer_received_date timestamp,
	designer_rejected char(1),
	designer_returned char(1),
	designer_not_accepted_reason text,
	PRIMARY KEY (dye_custom_order_id)
) WITHOUT OIDS;
