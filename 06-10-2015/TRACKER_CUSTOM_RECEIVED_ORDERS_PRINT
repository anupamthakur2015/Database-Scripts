CREATE TABLE TRACKER_CUSTOM_RECEIVED_ORDERS_PRINT
(
	print_custom_order_id serial NOT NULL UNIQUE,
	order_id int NOT NULL,
	print_length float NOT NULL,
	print_width float NOT NULL,
	print_pcs int NOT NULL,
	print_total_measurement float NOT NULL,
	received_date timestamp NOT NULL,
	designer_received char(1),
	designer_received_date timestamp,
	designer_rejected char(1),
	designer_returned char(1),
	designer_not_accepted_reason text,
	PRIMARY KEY (print_custom_order_id)
) WITHOUT OIDS;
